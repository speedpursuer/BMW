doctype html
html
    head
        title= title
        //script(type='text/javascript' src="https://cdn.bootcss.com/jquery/2.1.1-rc2/jquery.js")
        link(rel='stylesheet', href='https://cdn.bootcss.com/bootstrap/3.3.7/css/bootstrap.min.css')
    body
        .container-fluid
            table(class="table")
                caption 当前收益
                thead
                    tr
                        th 名称
                        th 开始时间
                        th 更新时间
                        th 次数
                        th 收益
                tbody
                    each row, i in data
                        tr
                            th #{row.name}
                            th #{row.lastUpdate}
                            th #{row.startTime}
                            th #{row.tradeTimes}
                            th #{row.profit}
